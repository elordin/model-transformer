<?xml version="1.0" encoding="UTF-8"?>
<behaviour:Behaviour
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:behaviour="http://www.example.org/behaviour"
    xsi:schemaLocation="http://www.example.org/behaviour ../behaviour.ecore">
  <block>
    <preState
        name="WaitVerify"/>
    <postState
        name="WaitVerify"/>
    <inmessage>
      <operation
          name="verifyAccount"/>
    </inmessage>
    <activation>
      <outmessages>
        <object
            xsi:type="behaviour:SystemObject"
            name="bank"/>
        <operation
            name="verifyCardWithBank"/>
        <returned
            xsi:type="behaviour:Value"
            value="badBankAccount"/>
      </outmessages>
      <returned
          xsi:type="behaviour:Value"
          value="badAccount"/>
    </activation>
  </block>
  <systemobject
      name="consortium"/>
</behaviour:Behaviour>
