        prestate="//@scenario.0/@state.0"
        poststate="//@scenario.0/@state.1">
      <returned
          xsi:type="mdd_1:VoidValue"/>
      <systemmessage
          name="insertCard"
          sender="//@scenario.0/@actor.0"
          receiver="//@scenario.0/@actor.1">
